{
  "name": "@sosie-js/view-plugin",
  "version": "4.0.0",
  "keywords": [
    "sosie",
    "view-plugin",
    "sosiejs"
  ],
  "description": "View plugin for SoSIE.js",
  "license": "MIT",
  "repository": "https://github.com/sosie-js/plugin",
  "main": "./dist/bundle.js",
  "scripts": {
    "build": "yarn install;rimraf dist/*;yarn download;node minify.js;webpack --mode production",
    "build:dev": "yarn install;rimraf dist/*;yarn download;cp src/index.js dist/bundle.js; cp src/sample.js dist/sample.js;webpack --mode development --watch",
    "download": "node ./tasks/download.js",
    "lint": "eslint src/ --ext .js",
    "lint:errors": "eslint src/ --ext .js --quiet",
    "lint:fix": "eslint src/ --ext .js --fix",
    "lint:init": "eslint src/ --ext .js --init"
  },
  "author": {
    "name": "sosie",
    "email": "sosie@sos-productions.com"
  },
  "dependencies": {
    "@node-minify/core": "^6.0.0",
    "@node-minify/terser": "^6.0.0",
    "eslint": "^7.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "empty": "^0.10.1",
    "jsondiffpatch": "^0.4.1",
    "webpack": "^5.4.0",
    "webpack-cli": "^4.2.0"
  }
}
