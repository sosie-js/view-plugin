{
  "name": "@sosie-js/view-plugin",
  "version": "3.0.0",
  "keywords": [
    "sosie",
    "view-plugin",
    "sosiejs"
  ],
  "description": "View plugin for SoSIE.js",
  "license": "MIT",
  "repository": "https://github.com/sosie-js/plugin",
  "main": "./dist/bundle.js",
  "scripts": {
    "build": "yarn install;rimraf dist/*;node ./tasks/download.js;node minify.js",
    "build:dev": "yarn install;rimraf dist/*;node ./tasks/download.js;cp src/index.js dist/bundle.js; cp src/sample.js dist/sample.js;node minify-dev.js",
    "lint": "eslint src/ --ext .js",
    "lint:errors": "eslint src/ --ext .js --quiet",
    "lint:fix": "eslint src/ --ext .js --fix",
    "lint:init": "eslint src/ --ext .js --init"
  },
  "author": {
    "name": "sosie",
    "email": "sosie@sos-productions.com"
  },
  "dependencies": {
    "@node-minify/core": "^6.0.0",
    "@node-minify/terser": "^6.0.0",
    "eslint": "^7.11.0"
  },
  "devDependencies": {
    "jsondiffpatch": "^0.4.1"
  }
}
